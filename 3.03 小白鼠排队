#include<iostream>
#include<cstdio>
#include<algorithm>
using namespace std;

const int maxn = 100;

struct mouse{
    int weight;
    string color;
};

bool comp(const mouse &a, const mouse &b){
    return a.weight>b.weight;
}
int main(){
    int n;
    while(cin>>n){
        mouse num[maxn];
        for(int i=0;i<n;i++){
            cin>>num[i].weight>>num[i].color;
        }
        sort(num,num+n,comp);//数组的比较方法一定要注意
        for(int i=0;i<n;i++){
            cout<<num[i].color<<endl;
        }
    }
    return 0;
}

